<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <title>Events</title>
  <link rel="stylesheet" href="css/bootstrap.min.css">
  <!-- PWA Setup Início -->
  <link rel="manifest" href="manifest.json" />
  <!-- ios support -->
  <link rel="apple-touch-icon" href="images/icons/icon-72x72.png" />
  <link rel="apple-touch-icon" href="images/icons/icon-96x96.png" />
  <link rel="apple-touch-icon" href="images/icons/icon-128x128.png" />
  <link rel="apple-touch-icon" href="images/icons/icon-144x144.png" />
  <link rel="apple-touch-icon" href="images/icons/icon-152x152.png" />
  <link rel="apple-touch-icon" href="images/icons/icon-192x192.png" />
  <link rel="apple-touch-icon" href="images/icons/icon-384x384.png" />
  <link rel="apple-touch-icon" href="images/icons/icon-512x512.png" />
  <meta name="apple-mobile-web-app-status-bar" content="#db4938" />
  <meta name="theme-color" content="#db4938" />
  <!-- PWA Setup Fim -->
</head>

<body>
  <main>
    <div class="container">
      <h1 class="text-center mt-2 mb-5">EVENTS FROM EVENTBRITE SITE</h1>
      <div class="row m-auto">
        <% Object.keys(events).forEach(function(prop) { %>
          <div class="col mb-2">
            <div class="card" style="width: 18rem;">
              <img src="<%-events[prop].image%>" class="card-img-top" alt="Event image">
              <div class="card-body">
                <h5 class="card-title">Evento: <%-events[prop].name%></h5>
                <p class="card-text">De: <%-events[prop].startDate%> Até: <%-events[prop].endDate%></p>
                <p class="crad-text">Endereço: <%-events[prop].location.address.streetAddress%> - <%-events[prop].location.address.addressLocality%> - <%-events[prop].location.address.addressRegion%></p>
                <a class="btn btn-info btn-block" href="<%-events[prop].offers.url%>">Detalhes</a>
              </div>
            </div>
          </div>
        <% }); %>
      </div>
    </div>

    <script src="js/jquery-3.4.1.slim.min.js"></script>
    <script src="js/popper.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
  </main>
  <script>
    if ("serviceWorker" in navigator) {
      window.addEventListener("load", function() {
        navigator.serviceWorker
          .register("serviceWorker.js")
          .then(res => console.log("service worker registered"))
          .catch(err => console.log("service worker not registered", err))
      })
    }
  </script>
</body>
</html>
